	month = jun,
	year = {2006},
	pages = {1540--1542},
	file = {Full Text PDF:/home/quentin/.mozilla/firefox/sopdkna1.default/zotero/storage/A8DRER3S/Suzuki and Shimodaira - 2006 - Pvclust An R Package for Assessing the Uncertaint.pdf:application/pdf}
