@inproceedings{chengcui_zhang_effective_2007,
	title = {An Effective and Robust Method for Automatic Bacterial Colony Enumeration},
	isbn = {978-0-7695-2997-4},
	lccn = {0004},
	doi = {10.1109/ICSC.2007.20},
	abstract = {Bacterial colony enumeration has applications in many different assays such as antibiotic screening, toxicology testing, and genotoxicity measuring. The counting of bacterial colony is usually performed by well-trained technicians manually. However, this manual enumeration process has a very low throughput, and is time consuming and labor intensive in practice. To provide consistent and accurate results and improve the throughput, the existing colony counter devices and software were then developed and commercialized in the market. In this study, we propose a fully automatic colony counter and compare its performance with Clono-Counter, an existing automatic colony counter reported by Niyazi et al. Our proposed method can significantly reduce the manual labor by automatically detecting the dish/plate region and extracting and counting colonies. Our experimental results show that the proposed method outperforms Clono-Counter in terms of Precision, Recall, and F-measures.},
	booktitle = {International Conference on Semantic Computing, 2007. {ICSC} 2007},
	publisher = {{IEEE}},
	author = {Chengcui Zhang and Wei-Bang Chen},
	month = sep,
	year = {2007},
	keywords = {Antibiotics, antibiotic screening, automatic bacterial colony enumeration, automatic colony counter, biology computing, Commercialization, Counting circuits, Fluorescence, Laboratories, manual enumeration process, Microorganisms, Robustness, Shape, Throughput, Toxicology, toxicology testing},
	pages = {581--588},
	file = {IEEE Xplore Full Text PDF:/home/quentin/.mozilla/firefox/sopdkna1.default/zotero/storage/7SX5IFFP/Chengcui Zhang and Wei-Bang Chen - 2007 - An Effective and Robust Method for Automatic Bacte.pdf:application/pdf}
