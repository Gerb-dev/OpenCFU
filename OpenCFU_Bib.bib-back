	abstract = {Image processing is becoming more and more present in our everyday life. With the requirements of miniaturization, low-power, performance in order to provide some intelligent processing directly into the camera, embedded camera will dominate the image processing landscape in the future. While the common approach of developing such embedded systems is to use sequentially operating processors, image processing algorithms are inherently parallel, thus hardware devices like {FP-GAs} provide a perfect match to develop highly efficient systems. Unfortunately hardware development is more difficult and there are less experts available compared to software. Automatizing the design process will leverage the existing infrastructure, thus providing faster time to market and quick investigation of new algorithms. We exploit {ASP} (answer set programming) for system synthesis with the goal of genarating an optimal hardware software partitioning, a viable communication structure and the corresponding scheduling, from an image processing application.},
	booktitle = {2011 22nd {IEEE} International Symposium on Rapid System Prototyping {(RSP)}},
	publisher = {{IEEE}},
	author = {Muhlbauer, F. and Groshans, M. and Bobda, C.},
	month = may,
	year = {2011},
	keywords = {answer set programming, Computer architecture, Delay, embedded camera, embedded systems, field programmable gate arrays, {FPGA}, Hardware, hardware software partitioning, Image processing, {OpenCV} image processing, Program processors, rapid prototyping, Resource management, sequentially operating processors},
	pages = {16--22},
	file = {IEEE Xplore Full Text PDF:/home/quentin/.mozilla/firefox/sopdkna1.default/zotero/storage/5BT466WI/Muhlbauer et al. - 2011 - Rapid prototyping of OpenCV image processing appli.pdf:application/pdf}
},

